<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.petbook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bone_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MiTema">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.SecActivity"
            android:parentActivityName=".activities.SecActivity" />
        <activity android:name=".activities.FormularioContacto"
            android:parentActivityName=".activities.SecActivity" />
        <activity android:name=".activities.About"
            android:parentActivityName=".activities.SecActivity" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>